<div class="row">
  <h4>Hola <%=@admin.nombre%>,</h4>
</div>
<!--Notificaciones-->
<div class="row">
  <h5>Notificaciones:</h5>
  <div class="collection">
    <a href="#" class="collection-item">Alvin</a>
    <a href="#" class="collection-item">Alvin</a>
    <a href="#" class="collection-item">Alvin</a>
  </div>
</div>
<!--Eventos de la semana-->
<div class="row">
  <div class="col s12 m6">
   <h5>Eventos esta semana:</h5>
   <ul class="collection with-header">
    <li class="collection-header"><h6>FECHA</h6></li>
    <%@eventos.each do |evento|%>
    <% if evento.fechaInicio> @weekstart.at_beginning_of_week && evento.fechaInicio < @weekstart.at_end_of_week%>
    <li class="collection-item"><div><%=evento.nombre%><a href="/eventos/<%=evento.folio%>" class="secondary-content"><i class="material-icons">send</i></a></div></li>
    <%end%>
    <%end%>
  </ul>
</div>
<div class="col s12 m6">
 <h5>Eventos siguiente semana:</h5>
 <ul class="collection with-header">
  <li class="collection-header"><h6>FECHA</h6></li>
  <%@eventos.each do |evento|%>
  <% if evento.fechaInicio> @weekdos.at_beginning_of_week && evento.fechaInicio < @weekdos.at_end_of_week%>
  <li class="collection-item"><div><%=evento.nombre%><a href="/eventos/<%=evento.folio%>" class="secondary-content"><i class="material-icons">send</i></a></div></li>
  <%end%>
  <%end%>
</ul>
</div>
</div>
<!--Botones-->
<div class="row center">
  <%= link_to "Eventos", "/admin/eventos" , :class => 'waves-effect waves-light btn'%>
  <%= link_to "Grupos", "/admin/grupos" , :class => 'waves-effect waves-light btn'%>
  <%= link_to "Administradores", "/admin/admins" , :class => 'waves-effect waves-light btn'%>
</div>